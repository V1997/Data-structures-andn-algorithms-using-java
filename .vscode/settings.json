{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.autoSaveDelay": 5000,
    "editor.fontFamily": "open sans",
    "editor.tabSize": 6,
    "editor.fontSize": 12,
    "terminal.integrated.fontSize": 16,
    "editor.defaultFormatter": "ecmel.vscode-html-css",
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "editor.formatOnPaste": true
}